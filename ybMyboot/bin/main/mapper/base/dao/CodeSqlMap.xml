<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.google.ybMyboot.base.dao.CodeDAO">

	<select id="selectCode" resultType="com.google.ybMyboot.base.to.CodeTO">
		<![CDATA[
		SELECT CODE_NUMBER
		, CODE_NAME
		, MODIFIABLE 
		FROM DIVISION_CODE
		ORDER BY CODE_NUMBER
		]]>
	</select>
</mapper>